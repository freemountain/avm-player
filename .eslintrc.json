{
    "extends": "standard",
    "ecmaFeatures": {
        "jsx": true
    },
    "rules": {
      "semi": [2, "always"],
      "space-before-function-paren": [2, "never"],
      "space-after-keywords": [2, "never"],
      "curly": [0],
      "react/jsx-pascal-case": 1
    },
    "plugins": [
        "standard",
        "react"
    ]
}
